{
  "name": "nest-typescript-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "apidoc": {
    "name": "Ysapp API Documentation",
    "version": "1.0.0",
    "description": "API de l'application Ysapp. Cette documentation contient toutes les endpoints expos√© par l'API ainsi que le corps de requete",
    "title": "Ysapp API Documentation",
    "url": "https://ysapp-api.yaba-in.com",
    "sampleUrl": "https://ysapp-api.yaba-in.com"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "env-cmd -f .env.prod nest build",
    "build-dev": "env-cmd -f .env.dev nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f .env.dev nest start",
    "start:dev": "env-cmd -f .env.dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "doc": "apidoc -i src -o docs",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/websockets": "^9.4.2",
    "aws-sdk": "^2.1323.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "country-telephone-data": "^0.6.3",
    "cron": "^2.3.1",
    "env-cmd": "^10.1.0",
    "express-jwt-blacklist": "^1.1.0",
    "google-auth-library": "^8.7.0",
    "googleapis": "^118.0.0",
    "handlebars": "^4.7.7",
    "keycloak-connect": "^21.1.2",
    "mongodb": "^5.6.0",
    "mongoose": "^6.11.2",
    "nest-aws-sdk": "^3.0.1",
    "nest-keycloak-connect": "^1.9.2",
    "nestjs-command": "^3.1.3",
    "nodemailer": "^6.9.1",
    "papaparse": "^5.4.1",
    "passport": "^0.6.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "turndown": "^7.1.2",
    "whatsapp-web.js": "^1.23.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-google-oauth2": "^0.1.5",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
